	bool hasArrayTwoCandidates(int arr[], int n, int x) {
	    // code here
	    int sum=0;
	    for(int i=0; i<n ; i++)
	    { 
	        for(int j=i ; j< n ; j++)
	        { 
	            if(i!=j)
	            {
	                if(arr[i]+arr[j]==x)
	                {
	                    return true;
	                }
	            }
	            
	        }
	       
	    }
	    return false;
	}
