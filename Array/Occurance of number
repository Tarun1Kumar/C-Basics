int count(int arr[], int n, int x) {
	    // code here
	    int counter = 0 ;
	    int flg = 0;
	    for(int i  = 0  ; i <   n;  i++ )
	    {
	        if( arr[i]   == x   )
	        {
	            counter++;
	            flg=1;
	        }
	    }
	    if(flg==1){
	    return counter;}
	    else{
	        return 0;
	    }
	}
