pair<long long, long long> getMinMax(long long a[], int n) {
    
     int max= 0 ;
     int min = 1000000;
     for(int i = 0 ; i< n ; i++){
         if(max<=a[i])
         {
             max = a[i];
         }
         if(min>=a[i])
         {
             min = a[i];
         }
     }
     return {min,max};
  
}
